<?xml version='1.0' encoding='utf-8'?>
<widget id="hellototp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>TOTP Authenticator</name>
    <description>
        This is a custom auth app from Okta
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <feature name="Camera">
        <param name="ios-package" value="CDVCamera" />
    </feature>
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <universal-links>
        <host name="booons.com">
            <path url="/some/path/*" />
        </host>
        <host name="atkoflowers.com" />
    </universal-links>
    <platform name="ios">
        <icon height="57" platform="ios" src="www/logo.png" width="57" />
        <icon height="114" platform="ios" src="www/logo.png" width="114" />
        <icon height="40" platform="ios" src="www/logo.png" width="40" />
        <icon height="80" platform="ios" src="www/logo.png" width="80" />
        <icon height="50" platform="ios" src="www/logo.png" width="50" />
        <icon height="100" platform="ios" src="www/logo.png" width="100" />
        <icon height="60" platform="ios" src="www/logo.png" width="60" />
        <icon height="120" platform="ios" src="www/logo.png" width="120" />
        <icon height="180" platform="ios" src="www/logo.png" width="180" />
        <icon height="72" platform="ios" src="www/logo.png" width="72" />
        <icon height="144" platform="ios" src="www/logo.png" width="144" />
        <icon height="76" platform="ios" src="www/logo.png" width="76" />
        <icon height="29" platform="ios" src="www/logo.png" width="29" />
        <icon height="58" platform="ios" src="www/logo.png" width="58" />
        <icon height="87" platform="ios" src="www/logo.png" width="87" />
        <icon height="152" platform="ios" src="www/logo.png" width="152" />
    </platform>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <platform name="android">
         <allow-intent href="itms:*" />
         <allow-intent href="itms-apps:*" />
     </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="phonegap-plugin-barcodescanner" spec="~8.0.1" />
    <plugin name="cordova-plugin-dialogs" spec="~2.0.1" />
    <plugin name="cordova-plugin-inappbrowser" spec="~3.0.0" />
    <plugin name="cordova-plugin-camera" spec="~4.0.3" />
    <plugin name="cordova.plugins.diagnostic" spec="~4.0.12" />
    <plugin name="cordova-universal-links-plugin" spec="~1.2.1" />
</widget>
